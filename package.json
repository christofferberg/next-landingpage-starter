{
  "name": "next-landingpage-starter",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --cache --fix",
    "test:e2e": "playwright test",
